{
  "name": "react-rxjs-keyahead",
  "version": "0.1.0",
  "description": "React experiments with rxjs",
  "private": true,
  "main": "src/view-layer/_root/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "staging": "NODE_ENV=staging webpack",
    "test": "jest --watchAll",
    "format": "prettier --write 'src/**/*.js'",
    "lintjs": "eslint 'src/**/*.js' --fix",
    "lintstyles": "stylelint **/*.scss"
  },
  "lint-staged": {
    "*.{js,jsx,scss,json}": [
      "eslint --fix",
      "stylelint **/*.scss",
      "jest /src",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|png|gif|ttf|eot|svg)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "connected-react-router": "^6.3.1",
    "history": "^4.7.2",
